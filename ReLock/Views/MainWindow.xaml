<Window x:Class="ReLock.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:views="clr-namespace:ReLock.Views"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525" WindowState="Maximized" WindowStyle="None">
    <Grid Background="Black">
        <ContentControl prism:RegionManager.RegionName="ContentRegion" />
        <views:ButtonPanel Width="200" Height="200"></views:ButtonPanel>
    </Grid>
</Window>
